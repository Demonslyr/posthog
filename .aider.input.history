+    m = hashlib.sha256()
+    m.update(f"{organization_id}".encode())
+    m.update(domain.encode())
+    digest = m.hexdigest()[:20]
+    return f"{digest}.{settings.PROXY_BASE_CNAME}"
+
+
+class ProxyRecordSerializer(serializers.ModelSerializer):
